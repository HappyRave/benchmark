set title 'Benchmark of open, write, read, close'
set xlabel 'size [B] of the file at opening or length of the string to read/write'
set ylabel 'time [us]'
set key right bottom
set logscale x
plot 'sys_sync.csv' using 1:2 title 'sys_sync',\
  'sys_nosync.csv' using 1:2 title 'sys_nosync',\
  'sys_direct.csv' using 1:2 title 'sys_direct',\
  'std_buf.csv' using 1:2 title 'std_buf',\
  'std_nobuf.csv' using 1:2 title 'std_nobuf'
