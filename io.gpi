set title 'Benchmark of open, write, read, close'
set xlabel 'size [B] of the file at opening or length of the string to read/write'
set ylabel 'time [us]'
set key right bottom
set logscale y
set logscale x
plot 'write_nosync.csv' using 1:2 title 'write_nosync',\
  'read_nosync.csv' using 1:2 title 'read_nosync',\
  'write_sync.csv' using 1:2 title 'write_sync',\
  'read_sync.csv' using 1:2 title 'read_sync'
